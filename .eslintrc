{
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "max-len": ["error", 150, { "ignoreUrls": true }]
  }
}
